<h1>List of all directors</h1>

<table border="1">
 <tr>
    <th>Id</th>
    <th>Image</th>
    <th>Name</th>
    <th>Year</th>
    <th> </th>
  </tr>

<% @list_of_directors.each do |a_thing| %>
 <tr>
  <td><%= a_thing.id %></td>
  <td>
    <img src="http://robohash.org/<%= a_thing.name %>">
  </td>
  <td><%= a_thing.name %></td>
  <td><%= a_thing.dob %></td>
  <td>
    <a href="/directors/<%= a_thing.id %>">
    Show details
  </td>
 </tr>
<% end %>
</table>
